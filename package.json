  "homepage": "https://demo.atlas.bi",
  "license": "AGPL-3.0-or-later",
  "name": "atlas",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:atlas-bi/atlas-bi-library"
  },
  "scripts": {
    "start": "gulp run",
    "build": "gulp build",
    "watch": "gulp watch",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "format:static": "prettier --config .prettierrc \"**/*.{ts,css,less,scss,js,json,md,yaml,yml,html}\" --write",
    "format:cs": "dotnet csharpier .",
    "format": "npm run format:static & npm run format:cs",
    "dotnet:build": "dotnet build",
    "dotnet:restore": "dotnet restore",
    "test:report_html": "reportgenerator -reports:coverage.cobertura.xml -targetdir:coverage/ -reporttypes:html",
    "test": "coverlet web.Tests/bin/Debug/net6.0/web.Tests.dll --target \"dotnet\" --targetargs \"test  --no-build\" --format cobertura --exclude-by-file \"**/Migrations/*\"",
    "test:dev": "npm run dotnet:build && npm run test & npm run test:report_html"
  },
  "lint-staged": {
    "*.cs": "dotnet csharpier .",
    "**/*.{ts,css,less,scss,js,json,md,yaml,html}": "npm run format"
  },
  "dependencies": {
    "bulma-steps-component": "github:atlas-bi/bulma-steps",
    "markdown-it": "^12.3.2"
  }
}
