    "@fontsource/inter": "^4.5.7",
    "@fontsource/rasa": "^4.5.8",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "bulma": "^0.9.3",
    "bulma-checkradio": "^2.1.3",
    "bulma-switch": "^2.0.4",
    "chart.js": "2.6.0",
    "date-fns": "^2.28.0",
    "es6-symbol": "^3.1.3",
    "eslint": "^8.12.0",
    "fancy-log": "^2.0.0",
    "focus-within": "^3.0.2",
    "fontawesome-subset": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^4.1.3",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^7.0.4",
    "jsnlog": "^2.30.0",
    "lint-staged": "^12.3.7",
    "open": "^8.4.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "proxy-polyfill": "^0.3.2",
    "regenerator-runtime": "^0.13.9",
    "rollup-stream-gulp": "^0.0.3",
    "sass": "^1.49.9"
