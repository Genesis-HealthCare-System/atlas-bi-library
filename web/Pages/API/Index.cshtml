@page
@model Atlas_Web.Pages.API.IndexModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
<div>
    <h1>Dup Summary</h1>
    <table>
        @foreach (var i in ViewBag.summary)
        {
            <tr><td>@i.Key</td><td>@i.Cnt</td></tr>

        }
    </table>
    <h1>Duplicate Users</h1>
    <table>
        @foreach (var i in ViewBag.stuff)
        {
            <tr><td>@i.Key</td><td>@i.Cnt</td></tr>

        }
    </table>
   
    <h1>All Users</h1>
    <table>
        @foreach (var i in ViewBag.alluserrs)
        {
            <tr><td>@i.Key</td></tr>
        }
    </table>
</div>
